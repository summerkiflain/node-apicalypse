    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.0.0-rc.2",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^2.6.2",
    "mocha": "^5.2.0",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "axios-retry": "^3.1.9",
    "better-queue": "^3.8.10",
    "better-queue-memory": "^1.0.3"
  }
